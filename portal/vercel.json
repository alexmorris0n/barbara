{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci || npm install",
  "rewrites": [
    {
      "source": "/api/test-call/:match*",
      "destination": "https://barbara-cli-testing.fly.dev/api/test-call/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

